<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="1.0" vgap="3.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
        <ColumnConstraints />
        <ColumnConstraints />
        <ColumnConstraints />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <children>
        <TextField GridPane.columnIndex="2" GridPane.rowIndex="1" />
        <Label fx:id="idLabel" text="Usuario:" GridPane.columnIndex="1">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding></Label>
        <Label text="Contraseña:" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding></Label>
        <Button fx:id="idBoton" mnemonicParsing="false" onAction="#contboton" text="Guardar" GridPane.columnIndex="2" GridPane.rowIndex="4">
            <GridPane.margin>
                <Insets left="50.0" />
            </GridPane.margin></Button>
        <TextArea maxHeight="1.7976931348623157E308" prefHeight="1.0" prefWidth="3.0" GridPane.columnIndex="2">
            <GridPane.margin>
                <Insets />
            </GridPane.margin></TextArea>
        <Label fx:id="idLabel2" text="Mostrar Contraseña" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </Label>
        <PasswordField fx:id="password" GridPane.columnIndex="2" GridPane.rowIndex="1">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding></PasswordField>
    </children>
</GridPane>
